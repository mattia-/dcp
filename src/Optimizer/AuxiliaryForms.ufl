# This file contains auxiliary forms used by the optimizer
element = FiniteElement ("CG", triangle, 1)                                       

# ------------------------------------------- #
# define form to compute L^2 norm of a function
# ------------------------------------------- #
# the function whose norm we wish to compute
u = Coefficient (element)                                                        

# the form itself
NormComputer = (u * u) * dx
forms = [NormComputer]  
