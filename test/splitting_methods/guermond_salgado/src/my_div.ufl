# fourth step of the Guermond-Salgado splitting method

# domain type
cell = triangle

# function spaces
V = FiniteElement ("Lagrange", cell, 1)
W = VectorElement ("Lagrange", cell, 2)

# coefficients
vel = Coefficient (W)
    
# test and trial functions
u = TrialFunction (V)
v = TestFunction (V)

# pressure update equation
# lhs
a = inner (u, v) * dx

# rhs
L = inner (div (vel), v) * dx

